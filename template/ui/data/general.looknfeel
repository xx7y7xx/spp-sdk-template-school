<?xml version="1.0" encoding="utf-8"?>
<Falagard xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Falagard.xsd">
	<!--
		*****************************************************************************
		*DefaultWindowBackground
		*****************************************************************************
	-->
	<WidgetLook name="General/DefaultWindowBackground">
		<PropertyDefinition name="font" initialValue="You14" redrawOnWrite="true"/>
		<PropertyDefinition name="text_theme" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="text_colour" initialValue="FFB5EDFC" redrawOnWrite="true"/>
		<PropertyDefinition name="font_vert" initialValue="TopAligned" redrawOnWrite="true"/>
		<PropertyDefinition name="font_horz" initialValue="LeftAligned" redrawOnWrite="true"/>
		<ImagerySection name="sectionBackground">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<Image type="TopLeftCorner" image="zhu_titleleft" imageset="zhucaidan"/>
				<Image type="TopEdge" image="zhu_titlemiddle" imageset="zhucaidan"/>
				<Image type="TopRightCorner" image="zhu_titleright" imageset="zhucaidan"/>
				<Image type="LeftEdge" image="zhu_middleleft" imageset="zhucaidan"/>
				<Image type="RightEdge" image="zhu_middleright" imageset="zhucaidan"/>
				<Image type="BottomLeftCorner" image="zhu_footleft" imageset="zhucaidan"/>
				<Image type="BottomEdge" image="zhu_footmiddle" imageset="zhucaidan"/>
				<Image type="BottomRightCorner" image="zhu_footright" imageset="zhucaidan"/>
				<Image type="Background" image="zhu_middlemiddle" imageset="zhucaidan"/>
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="sectionText">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="50"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="20"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<TextProperty name="text_theme"/>
				<FontProperty name="font"/>
				<ColourProperty name="text_colour"/>
				<VertFormatProperty name="font_vert"/>
				<HorzFormatProperty name="font_horz"/>
			</TextComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="sectionBackground"/>
				<Section section="sectionText"/>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="sectionBackground">
					<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				</Section>
				<Section section="sectionText">
					<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>
	<!--
		*****************************************************************************
