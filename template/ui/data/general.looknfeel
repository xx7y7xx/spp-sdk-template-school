	<!--
		*****************************************************************************
		*StaticImage
		*****************************************************************************
	-->
	<WidgetLook name="General/StaticImage">
		<PropertyDefinition name="image" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="vert_format" initialValue="Stretched" redrawOnWrite="true"/>
		<PropertyDefinition name="horz_format" initialValue="Stretched" redrawOnWrite="true"/>
		<ImagerySection name="noframe_image">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<ImageProperty name="image"/>
				<VertFormatProperty name="vert_format"/>
				<HorzFormatProperty name="horz_format"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="withframe_image">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<ImageDim image="zhu_middleleft" dimension="Width" imageset="zhucaidan"/>
					</Dim>
					<Dim type="TopEdge">
						<ImageDim image="zhu_titlemiddle" dimension="Height" imageset="zhucaidan"/>
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim type="RightEdge" scale="1">
							<DimOperator op="Subtract">
								<ImageDim image="zhu_middleright" dimension="Width" imageset="zhucaidan"/>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim type="BottomEdge" scale="1">
							<DimOperator op="Subtract">
								<ImageDim image="zhu_footmiddle" dimension="Height" imageset="zhucaidan"/>
							</DimOperator>
						</UnifiedDim>
					</Dim>
				</Area>
				<ImageProperty name="image"/>
				<VertFormatProperty name="vert_format"/>
				<HorzFormatProperty name="horz_format"/>
			</ImageryComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="noframe_image"/>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="noframe_image"/>
			</Layer>
		</StateImagery>
		<StateImagery name="EnabledFrame">
			<Layer>
				<Section section="frame" look="General/StaticShara"/>
			</Layer>
		</StateImagery>
		<StateImagery name="DisabledFrame">
			<Layer>
				<Section section="frame" look="General/StaticShara">
					<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="WithFrameEnabledBackground">
			<Layer>
				<Section section="background"/>
			</Layer>
		</StateImagery>
		<StateImagery name="WithFrameDisabledBackground">
			<Layer>
				<Section section="background" look="General/StaticShara">
					<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="NoFrameDisabledBackground">
			<Layer>
				<Section section="noframe_background">
					<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="NoFrameEnabledBaclground">
			<Layer>
				<Section section="noframe_background"/>
			</Layer>
		</StateImagery>
		<StateImagery name="WithFrameImage">
			<Layer>
				<Section section="withframe_image"/>
			</Layer>
		</StateImagery>
		<StateImagery name="NoFrmaeImage">
			<Layer>
				<Section section="noframe_image"/>
			</Layer>
		</StateImagery>
	</WidgetLook>
	<!--
		*****************************************************************************
		*General/StaticShara
		*****************************************************************************
	-->
	<WidgetLook name="General/StaticShara">
		<ImagerySection name="frame">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<Image type="TopLeftCorner" image="zhu_titleleft" imageset="zhucaidan"/>
				<Image type="TopEdge" image="zhu_titlemiddle" imageset="zhucaidan"/>
				<Image type="TopRightCorner" image="zhu_titleright" imageset="zhucaidan"/>
				<Image type="LeftEdge" image="zhu_middleleft" imageset="zhucaidan"/>
				<Image type="RightEdge" image="zhu_middleright" imageset="zhucaidan"/>
				<Image type="BottomLeftCorner" image="zhu_footleft" imageset="zhucaidan"/>
				<Image type="BottomEdge" image="zhu_footmiddle" imageset="zhucaidan"/>
				<Image type="BottomRightCorner" image="zhu_footright" imageset="zhucaidan"/>
				<Image type="Background" image="zhu_middlemiddle" imageset="zhucaidan"/>
				<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="background">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<ImageDim image="zhu_middleleft" dimension="Width" imageset="zhucaidan"/>
					</Dim>
					<Dim type="TopEdge">
						<ImageDim image="zhu_titlemiddle" dimension="Height" imageset="zhucaidan"/>
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim type="RightEdge" scale="1">
							<DimOperator op="Subtract">
								<ImageDim image="zhu_middleright" dimension="Width" imageset="zhucaidan"/>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim type="BottomEdge" scale="1">
							<DimOperator op="Subtract">
								<ImageDim image="zhu_footmiddle" dimension="Height" imageset="zhucaidan"/>
							</DimOperator>
						</UnifiedDim>
					</Dim>
				</Area>
				<Image image="zhu_middlemiddle" imageset="zhucaidan"/>
				<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				<VertFormat type="Stretched"/>
				<HorzFormat type="Stretched"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="noframe_background">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<Colours topRight="FFDADADA" topLeft="FFDADADA" bottomLeft="FFDADADA" bottomRight="FFDADADA"/>
				<Image image="zhu_middlemiddle" imageset="zhucaidan"/>
				<VertFormat type="Stretched"/>
				<HorzFormat type="Stretched"/>
			</ImageryComponent>
		</ImagerySection>
	</WidgetLook>
	<!--
		*****************************************************************************
		*Button
		*****************************************************************************
	-->
	<WidgetLook name="General/Button">
		<PropertyDefinition name="font" initialValue="You8" redrawOnWrite="true"/>
		<PropertyDefinition name="text_theme" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="text_vert_format" initialValue="CentreAligned" redrawOnWrite="true"/>
		<PropertyDefinition name="text_horz_format" initialValue="CentreAligned" redrawOnWrite="true"/>
		<PropertyDefinition name="NormalTextColour" initialValue="FF868485" redrawOnWrite="true"/>
		<PropertyDefinition name="HoverTextColour" initialValue="FFABA9A9" redrawOnWrite="true"/>
		<PropertyDefinition name="PushedTextColour" initialValue="FF515151" redrawOnWrite="true"/>
		<PropertyDefinition name="DisabledTextColour" initialValue="FFF9C9C1" redrawOnWrite="true"/>
		<PropertyDefinition name="NormalImage" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="HoverImage" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="PushedImage" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="image_vert_format" initialValue="Stretched" redrawOnWrite="true"/>
		<PropertyDefinition name="image_horz_format" initialValue="Stretched" redrawOnWrite="true"/>
		<PropertyDefinition name="button_bg" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_image" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_bg_width" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_bg_height" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_image_width" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_image_height" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_width" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_height" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="DisabledImage" initialValue="" redrawOnWrite="true"/>
		<PropertyDefinition name="button_image_vertformat" initialValue="Stretched" redrawOnWrite="true"/>
		<PropertyDefinition name="button_image_horzformat" initialValue="Stretched" redrawOnWrite="true"/>
		<ImagerySection name="label">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<TextProperty name="text_theme"/>
				<FontProperty name="font"/>
				<VertFormatProperty name="text_vert_format"/>
				<HorzFormatProperty name="text_horz_format"/>
			</TextComponent>
		</ImagerySection>
		<ImagerySection name="normal">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<ImageProperty name="NormalImage"/>
				<VertFormat type="CentreAligned"/>
				<HorzFormat type="CentreAligned"/>
				<VertFormatProperty name="image_vert_format"/>
				<HorzFormatProperty name="image_horz_format"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="hover">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<ImageProperty name="HoverImage"/>
				<VertFormat type="CentreAligned"/>
				<HorzFormat type="CentreAligned"/>
				<VertFormatProperty name="image_vert_format"/>
				<HorzFormatProperty name="image_horz_format"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="pushed">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<ImageProperty name="PushedImage"/>
				<VertFormat type="CentreAligned"/>
				<HorzFormat type="CentreAligned"/>
				<VertFormatProperty name="image_vert_format"/>
				<HorzFormatProperty name="image_horz_format"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="disabled">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="Width">
						<UnifiedDim type="Width" scale="1"/>
					</Dim>
					<Dim type="Height">
						<UnifiedDim type="Height" scale="1"/>
					</Dim>
				</Area>
				<ImageProperty name="DisabledImage"/>
				<VertFormat type="CentreAligned"/>
				<HorzFormat type="CentreAligned"/>
				<VertFormatProperty name="image_vert_format"/>
				<HorzFormatProperty name="image_horz_format"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="button_bg">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0.5">
							<DimOperator op="Multiply">
								<WidgetDim dimension="Width">
									<DimOperator op="Subtract">
										<WidgetDim dimension="Width">
											<DimOperator op="Multiply">
												<PropertyDim name="button_bg_width">
													<DimOperator op="Divide">
														<PropertyDim name="button_width"/>
													</DimOperator>
												</PropertyDim>
											</DimOperator>
										</WidgetDim>
									</DimOperator>
								</WidgetDim>
							</DimOperator>
						</AbsoluteDim>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0.5">
							<DimOperator op="Multiply">
								<WidgetDim dimension="Height">
									<DimOperator op="Subtract">
										<WidgetDim dimension="Height">
											<DimOperator op="Multiply">
												<PropertyDim name="button_bg_height">
													<DimOperator op="Divide">
														<PropertyDim name="button_height"/>
													</DimOperator>
												</PropertyDim>
											</DimOperator>
										</WidgetDim>
									</DimOperator>
								</WidgetDim>
							</DimOperator>
						</AbsoluteDim>
					</Dim>
					<Dim type="Width">
						<WidgetDim dimension="Width">
							<DimOperator op="Multiply">
								<PropertyDim name="button_bg_width">
									<DimOperator op="Divide">
										<PropertyDim name="button_width"/>
									</DimOperator>
								</PropertyDim>
							</DimOperator>
						</WidgetDim>
					</Dim>
					<Dim type="Height">
						<WidgetDim dimension="Height">
							<DimOperator op="Multiply">
								<PropertyDim name="button_bg_height">
									<DimOperator op="Divide">
										<PropertyDim name="button_height"/>
									</DimOperator>
								</PropertyDim>
							</DimOperator>
						</WidgetDim>
					</Dim>
				</Area>
				<ImageProperty name="button_bg"/>
				<VertFormat type="Stretched"/>
				<HorzFormat type="Stretched"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="button_image">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0.5">
							<DimOperator op="Multiply">
								<WidgetDim dimension="Width">
									<DimOperator op="Subtract">
										<WidgetDim dimension="Width">
											<DimOperator op="Multiply">
												<PropertyDim name="button_image_width">
													<DimOperator op="Divide">
														<PropertyDim name="button_width"/>
													</DimOperator>
												</PropertyDim>
											</DimOperator>
										</WidgetDim>
									</DimOperator>
								</WidgetDim>
							</DimOperator>
						</AbsoluteDim>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0.5">
							<DimOperator op="Multiply">
								<WidgetDim dimension="Height">
									<DimOperator op="Subtract">
										<WidgetDim dimension="Height">
											<DimOperator op="Multiply">
												<PropertyDim name="button_image_height">
													<DimOperator op="Divide">
														<PropertyDim name="button_height"/>
													</DimOperator>
												</PropertyDim>
											</DimOperator>
										</WidgetDim>
									</DimOperator>
								</WidgetDim>
							</DimOperator>
						</AbsoluteDim>
					</Dim>
					<Dim type="Width">
						<WidgetDim dimension="Width">
							<DimOperator op="Multiply">
								<PropertyDim name="button_image_width">
									<DimOperator op="Divide">
										<PropertyDim name="button_width"/>
									</DimOperator>
								</PropertyDim>
							</DimOperator>
						</WidgetDim>
					</Dim>
					<Dim type="Height">
						<WidgetDim dimension="Height">
							<DimOperator op="Multiply">
								<PropertyDim name="button_image_height">
									<DimOperator op="Divide">
										<PropertyDim name="button_height"/>
									</DimOperator>
								</PropertyDim>
							</DimOperator>
						</WidgetDim>
					</Dim>
				</Area>
				<ImageProperty name="button_image"/>
				<VertFormatProperty name="button_image_vertformat"/>
				<HorzFormatProperty name="button_image_horzformat"/>
			</ImageryComponent>
		</ImagerySection>
		<StateImagery name="Normal">
			<Layer>
				<Section section="normal"/>
				<Section section="button_bg"/>
				<Section section="button_image"/>
				<Section section="label">
					<ColourProperty name="NormalTextColour"/>
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Hover">
			<Layer>
				<Section section="hover"/>
				<Section section="button_bg"/>
				<Section section="button_image"/>
				<Section section="label">
					<ColourProperty name="HoverTextColour"/>
